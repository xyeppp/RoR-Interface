<Interface
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
	<Windows>
		<Window name="Vote_Count_Template" sticky="false" handleinput="false" layer="default">
			<Anchors>
				<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
					<AbsPoint x="5" y="5" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsPoint x="25"y="25" />
			</Size>
			<Windows>
				<FullResizeImage name="$parentBG2" inherits="EA_FullResizeImage_TintableSolidBackground" layer="background" alpha="1" handleinput="false">
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="0" y="0"/>
						</Anchor>
						<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
							<AbsPoint x="0" y="0"/>
						</Anchor>
					</Anchors>
					<TintColor r="0" g="0" b="0" a="1"/>
				</FullResizeImage>
				<FullResizeImage name="$parentBG" inherits="EA_FullResizeImage_TintableSolidBackground" layer="default" alpha="1" handleinput="false">
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="2" y="2"/>
						</Anchor>
						<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
							<AbsPoint x="-2" y="-2"/>
						</Anchor>
					</Anchors>
					<TintColor r="125" g="125" b="125" a="1"/>
				</FullResizeImage>
			</Windows>
		</Window>
		<Window name="RoR_Window_ScenarioSurrender" savesettings="true" layer="default">
			<EventHandlers>
				<EventHandler event="OnUpdate" function="RoR_Window_ScenarioSurrender.Update" />
			</EventHandlers>
			<Anchors>
				<Anchor point="left" relativePoint="left" relativeTo="Root">
					<AbsPoint x="5" y="5" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsPoint x="350" y="150" />
			</Size>
			<Windows>
				<FullResizeImage name="$parentFrame" inherits="EA_FullResizeImage_DefaultFrame" layer="beckground"  >
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="0" y="0" />
						</Anchor>
						<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
							<AbsPoint x="0" y="0" />
						</Anchor>
					</Anchors>
				</FullResizeImage>
				<Label name="$parentText" textalign="center" font="font_default_text" layer="popup" handleinput="false">
					<Size>
						<AbsPoint x="340" y="42" />
					</Size>
					<Anchors>
						<Anchor point="top" relativePoint="top" relativeTo="$parent">
							<AbsPoint x="0" y="2" />
						</Anchor>
					</Anchors>
					<Color r="255" g="255" b="255" a="255" />
				</Label>
				<FullResizeImage name="$parentFrameBG" inherits="EA_FullResizeImage_TintableSolidBackground" layer="background" alpha="0.8" handleinput="false">
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
							<AbsPoint x="2" y="1"/>
						</Anchor>
						<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
							<AbsPoint x="-2" y="0"/>
						</Anchor>
					</Anchors>
					<TintColor r="20" g="20" b="55" a="0.8"/>
				</FullResizeImage>
				<StatusBar name="$parentBar1" foreground="EA_StatusBar_DefaultTintableBar" reverseFill="false" interpolate="false">
					<Size>
						<AbsPoint x="22" y="13" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parentTimerBar">
							<AbsPoint x="0" y="0" />
						</Anchor>
						<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentTimerBar">
							<AbsPoint x="0" y="0" />
						</Anchor>
					</Anchors>
				</StatusBar>
				<Window name="$parentTimerBar" inherits="EA_Window_DefaultFrameStatusBar" alpha="0" >
					<Size>
						<AbsPoint x="310" y="15" />
					</Size>
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent" >
							<AbsPoint x="20" y="71" />
						</Anchor>
					</Anchors>
				</Window>
				<FullResizeImage name="$parentBG" inherits="EA_FullResizeImage_TintableSolidBackground" layer="background" alpha="0.8" handleinput="false">
					<Anchors>
						<Anchor point="topleft" relativePoint="topleft" relativeTo="$parentTimerBar">
							<AbsPoint x="-1" y="0"/>
						</Anchor>
						<Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentTimerBar">
							<AbsPoint x="1" y="1"/>
						</Anchor>
					</Anchors>
					<TintColor r="0" g="0" b="0" a="0.8"/>
				</FullResizeImage>
				<Window name="$parent_Choice" sticky="true" handleinput="true" layer="overlay">
					<Size>
						<AbsPoint x="350" y="100" />
					</Size>
					<Anchors>
						<Anchor point="bottomleft" relativePoint="bottomleft">
							<AbsPoint x="0" y="0" />
						</Anchor>
					</Anchors>
					<Windows>
						<Button name="$parent_Vote_Yes" inherits="EA_Button_DefaultResizeable" id="1" textalign="center">
							<Size>
								<AbsPoint x="140" y="40" />
							</Size>
							<Anchors>
								<Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
									<AbsPoint x="20" y="43" />
								</Anchor>
							</Anchors>
							<EventHandlers>
								<EventHandler event="OnLButtonUp" function="RoR_Window_ScenarioSurrender.SelectChoice"/>
							</EventHandlers>
						</Button>
						<Button name="$parent_Vote_No" inherits="EA_Button_DefaultResizeable" id="2" textalign="center">
							<Size>
								<AbsPoint x="140" y="40" />
							</Size>
							<Anchors>
								<Anchor point="topright" relativePoint="topright" relativeTo="$parent">
									<AbsPoint x="-20" y="43" />
								</Anchor>
							</Anchors>
							<EventHandlers>
								<EventHandler event="OnLButtonUp" function="RoR_Window_ScenarioSurrender.SelectChoice"/>
							</EventHandlers>
						</Button>
						<Label name="$parentTimer" textalign="center" font="font_default_text" layer="popup" handleinput="false">
							<Size>
								<AbsPoint x="360" y="42" />
							</Size>
							<Anchors>
								<Anchor point="bottom" relativePoint="bottom" relativeTo="$parent">
									<AbsPoint x="0" y="-20" />
								</Anchor>
							</Anchors>
							<Color r="255" g="255" b="255" a="255" />
						</Label>
					</Windows>
				</Window>
			</Windows>
		</Window>
	</Windows>
</Interface>

