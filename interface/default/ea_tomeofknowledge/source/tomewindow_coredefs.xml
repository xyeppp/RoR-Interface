	    <Window name="TomeTOCItemWindowWide" inherits="TomeTOCItemWindow" >
	        <Size>
	            <AbsPoint x="370" y="20"/>
            </Size>
        </Window>
  

        <Button name="TomeToggleButton" inherits="EA_Button_Default" backgroundtexture="EA_Tome" highlighttexture="EA_Tome" drawchildrenfirst="true">
            <Size>
                <AbsPoint x="34" y="25" />
            </Size>          
            <TexSlices>
              <NormalHighlit id="Checkbox-Rollover" />
              <Pressed id="Check" />
              <PressedHighlit id="Checkbox-Rollover" />
            </TexSlices>
            <Windows>
              <DynamicImage name="$parentBox" texture="EA_Tome" slice="Checkbox" handleinput="false" >
                <Size>
                  <AbsPoint x="34" y="25" />
                </Size>
              </DynamicImage>
            </Windows>
        </Button>

        
        
    </Windows>
</Interface>