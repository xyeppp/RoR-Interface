	            	<Size>
	                    <AbsPoint x="280" y="22"/>
                    </Size>
                    <Anchors>
				        <Anchor point="topleft" relativePoint="topleft" relativeTo="">
					        <AbsPoint x="0" y="0"/>
				        </Anchor>
			        </Anchors>
	            </Button>
	            <Label name="$parentNumber" inherits="TOKText" handleinput="false" wordwrap="false" autoresize="false" textalign="left"  >
