        <!--# Button: EA_Button_DefaultIcon
            This is the default icon button.
            
            Code:
            (begin code)
             <Button name="EA_Button_DefaultIcon" inherits="EA_Button_Default" texturescale="0.859" font="font_default_text" textalign="bottomright" drawchildrenfirst="true">
                ...
            </Button>
            (end)            
        -->
        <Button name="EA_Button_DefaultIcon" inherits="EA_Button_Default" texturescale="0.859" font="font_default_text" textalign="bottomright" drawchildrenfirst="true">
            <Size>
                <AbsPoint x="55" y="55" />
            </Size>
            <Windows>
                <DynamicImage name="$parentIconBase" textureScale="0.859" handleinput="false" layer="background">
                    <Size>
                        <AbsPoint x="55" y="55" />
                    </Size>
                </DynamicImage>
            </Windows>
            <TexCoords>
                <Normal texture="icon_frame_square" x="0" y="0" />
                <NormalHighlit texture="icon_frame_square_highlight" x="0" y="0" />
                <Pressed texture="icon_frame_square_pressed" x="0" y="0" />
                <Disabled texture="icon_frame_square" x="0" y="0" />
            </TexCoords>
            <TextOffset x="5" />
        </Button>

        <!--# Button: EA_Button_DefaultIconStack
            This is the default icon button that allows for stack counts to be added.
            
            Code:
            (begin code)
             <Button name="EA_Button_DefaultIconStack" inherits="EA_Button_Default" texturescale="0.859" font="font_default_text" textalign="bottomright" drawchildrenfirst="true">
                ...
            </Button>
            (end)            
        -->
        <Button name="EA_Button_DefaultIconStack" inherits="EA_Button_Default" texturescale="0.859" font="font_default_text" textalign="bottomright" drawchildrenfirst="true">
            <Size>
                <AbsPoint x="55" y="55" />
            </Size>
            <Windows>
                <DynamicImage name="$parentIconBase" textureScale="0.859" handleinput="false" layer="background">
                    <Size>
                        <AbsPoint x="55" y="55" />
                    </Size>
                </DynamicImage>
                <Label name="$parentTextBase" font="font_chat_text" maxchars="12" textalign="right" handleinput="false" autoresize="true" wordwrap="false" layer="secondary">
                    <Size>
                        <AbsPoint x="40" y="20" />
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topright">
                            <AbsPoint x="-5" y="5" />
                        </Anchor>
                    </Anchors>
					<Color r="255" g="255" b="255" a="255" />
                </Label>
			</Windows>
            <TexCoords>
                <Normal texture="icon_frame_square" x="0" y="0" />
                <NormalHighlit texture="icon_frame_square_highlight" x="0" y="0" />
                <Pressed texture="icon_frame_square_pressed" x="0" y="0" />
                <Disabled texture="icon_frame_square" x="0" y="0" />
            </TexCoords>
            <TextOffset x="5" />
        </Button>
		
