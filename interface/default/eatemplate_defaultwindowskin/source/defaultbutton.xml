<!--# Title: EA_Button_Default 

    This file contains all of the standard button templates that are used to create most of the Default UI Buttons.
    You can inherit from any of these definitions for your add ons.
-->

<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">

    <Scripts>
        <Script file="Source/DefaultButton.lua" />
    </Scripts>
    
    <Windows>

        <!-- Button Defs ************************************************************************************************-->



        <!--# Button: EA_Button_Default                   
            This is the default button definion that ONLY provides the click sounds. It provides no artwork
            or text. This should be the base for all other button definitions.
            
            Code:
            (begin code)
             <Button name="EA_Button_Default">
             ...
             </Button>
            (end) 
        -->
        <Button name="EA_Button_Default">
            <Sounds>
                <Sound event="OnLButtonDown" script="Sound.Play( Sound.BUTTON_CLICK )" />
                <Sound event="OnMouseOver" script="Sound.Play( Sound.BUTTON_OVER )" />
            </Sounds>
        </Button>


        <!--# Button: EA_Button_DefaultWindowClose                   
            This is the default close button used for all windows. This only provides the artwork - each window is 
            responsible for adding a click callback.
        -->
        <Button name="EA_Button_DefaultWindowClose" inherits="EA_Button_Default" backgroundtexture="shared_01" highlighttexture="shared_01" layer="popup">
            <Size>
                <AbsPoint x="37" y="37" />
            </Size>
            <Anchors>
                <Anchor point="topright" relativePoint="topright" relativeTo="">
                    <AbsPoint x="0" y="0" />
                </Anchor>
            </Anchors>
            <TexCoords>
                <Normal x="274" y="46" />
                <NormalHighlit x="274" y="83" />
                <Pressed x="274" y="120"  />
                <PressedHighlit x="274" y="120"  />
                <Disabled x="274" y="46" />
            </TexCoords>
        </Button>

        <!--# Button: EA_Button_DefaultText                   
            This is the default text button for all windows. It provides no artwork, only a font.
        -->
        <Button name="EA_Button_DefaultText" inherits="EA_Button_Default" font="font_default_sub_heading">
            <Size>
                <AbsPoint x="200" y="32" />
            </Size>
            <TextColors>
                <Normal r="255" g="204" b="102" a="255" />
                <NormalHighlit r="255" g="85" b="0" a="255" />
                <Pressed r="255" g="85" b="0" a="255" />
                <PressedHighlit r="255" g="85" b="0" a="255" />
                <Disabled r="92" g="92" b="92" a="255" />
            </TextColors>
        </Button>



        <!--# Button: EA_Button_DefaultSmallSquare
            This is the default small square button.
            
            Code:
            (begin code)
             <Button name="EA_Button_DefaultSmallSquare" backgroundtexture="shared_01" highlighttexture="shared_01">
                ...
            </Button>
            (end)            
        -->
        <Button name="EA_Button_DefaultSmallSquare" inherits="EA_Button_Default" backgroundtexture="shared_01" highlighttexture="shared_01">
            <Size>
                <AbsPoint x="26" y="26" />
            </Size>
            <TexCoords>
                <Normal x="168" y="43" />
                <NormalHighlit x="168" y="69" />
                <Pressed x="168" y="95" />
                <PressedHighlit x="168" y="95" />
                <Disabled x="142" y="95" />
            </TexCoords>
        </Button>

