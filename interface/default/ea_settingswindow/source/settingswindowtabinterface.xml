                    <EventHandler event="OnSelChanged" function="SettingsWindowTabInterface.SelectRows" />
                </EventHandlers>
            </ComboBox>
            <ComboBox name="$parentNumberButtons" inherits="EA_ComboBox_DefaultResizableTiny" >
                <Anchors>
                    <Anchor point="right" relativePoint="left" relativeTo="$parentRows" >
                        <AbsPoint x="80" y="0" />
                    </Anchor>
                </Anchors>
                <EventHandlers>
                    <EventHandler event="OnSelChanged" function="SettingsWindowTabInterface.SelectNumButtons" />
                </EventHandlers>
            </ComboBox>
            <ComboBox name="$parentVisibility" inherits="EA_ComboBox_DefaultResizable" >
                <Anchors>
                    <Anchor point="right" relativePoint="left" relativeTo="$parentNumberButtons" >
                        <AbsPoint x="50" y="0" />
                    </Anchor>
                </Anchors>
                <EventHandlers>
                    <EventHandler event="OnSelChanged" function="SettingsWindowTabInterface.SelectVisibility" />
                </EventHandlers>
            </ComboBox>
        </Windows>
    </Window>
    
    <!--*******************************************************************-->
