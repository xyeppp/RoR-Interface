          <AbsPoint x="0" y="0"/>
        </Anchor>
      </Anchors>
      <EventHandlers>
        <EventHandler event="OnInitialize" function="SettingsWindowTabInterface.Initialize"/>
      </EventHandlers>
      <Windows>
      
