        <!-- Used to display money rewards in the tome.  -->
        <Window name="MoneyTooltip" inherits="TooltipBase">
            <Size>
                <AbsPoint x="200" y="85" />
            </Size>
            <Windows>
                <Window name="$parentBackground" inherits="EA_Window_DefaultTooltipBackground"/>
                <Label name="$parentTitle" sticky="false" font="font_default_sub_heading" maxchars="128"  wordwrap="false" autoresizewidth="true" >
                    <Size>
                        <AbsPoint x="230" y="25" />
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="10" y="5" />
                        </Anchor>
                    </Anchors>
                    <Color r="255" g="204" b="102" a="255"/>
                </Label>
                <Window name="$parentMoney" inherits="MoneyFrame">
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentTitle" />
                    </Anchors>
                </Window>
            </Windows>
        </Window>
        
        

