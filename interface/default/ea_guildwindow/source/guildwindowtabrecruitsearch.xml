                          <ListColumn windowname="Description" variable="desc" format="wstring"/>
                          <ListColumn windowname="RankText" variable="rank" format="number"/>
                      </ListColumns>
                  </ListData>
              </ListBox>

              <Label name="$parentNoGuildsFoundText" inherits="GuildWindowSearchTextTemplate" autoresize="true">
                  <Anchors>
                      <Anchor point="topleft" relativePoint="topleft" relativeTo="$parentList" >
                          <AbsPoint x="10" y="10" />
                      </Anchor>
                      <Anchor point="topright" relativePoint="topright" relativeTo="$parentList" >
                          <AbsPoint x="-10" y="10" />
                      </Anchor>
                  </Anchors>
              </Label>

             <!-- Selected Profile -->
              <Window name="$parentSelectedProfile" inherits="GuildRecruitProfileTemplate" >
                  <Anchors>
                      <Anchor point="bottomright" relativePoint="bottomleft" relativeTo="$parentVertRule" >
                          <AbsPoint x="-10" y="-10" />
                      </Anchor>
                      <Anchor point="bottomright" relativePoint="topright" relativeTo="$parentHorizRule" >
                          <AbsPoint x="-10" y="-10" />
                      </Anchor>
                  </Anchors>
                  <EventHandlers>
                      <EventHandler event="OnLButtonDown" function="GuildWindowTabRecruit.OnLButtonDownSelectedProfile"/>
                  </EventHandlers>
              </Window>


          </Windows>
      </Window>

  </Windows>  
</Interface>
