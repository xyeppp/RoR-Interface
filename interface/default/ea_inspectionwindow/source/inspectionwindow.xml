                        <Label name="$parentHeader" inherits="EA_Label_DefaultSubHeading" textalign="center">
                            <Size>
                                <AbsPoint x="500" y="50" />
                            </Size>
                            <Anchors>
                                <Anchor point="top" relativePoint="top" relativeTo="$parent">
                                    <AbsPoint x="0" y="10" />
                                </Anchor>
                            </Anchors>
                        </Label>
                        <Window name="$parentAnchor">
                            <Anchors>
                                <Anchor point="top" relativePoint="top" relativeTo="$parentHeader">
                                    <AbsPoint x="0" y="50" />
                                </Anchor>
                            </Anchors>
                        </Window>
                    </Windows>
                </Window>
                
                <!-- TABS -->
                <Window name="$parentTabs">
                    <Size>
                        <AbsPoint x="545" y="40" />
                    </Size>
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentBackground">
                            <AbsPoint x="0" y="-8" />
                        </Anchor>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentBackground">
                            <AbsPoint x="-10" y="40" />
                        </Anchor>
                    </Anchors>
                    <Windows>
                        <Button name="$parentCharTab" inherits="EA_Button_BottomTab">
                            <Anchors>
                                <Anchor point="topright" relativePoint="topright" relativeTo="">
                                    <AbsPoint x="0" y="0" />
                                </Anchor>
                            </Anchors>
                            <EventHandlers>
                                <EventHandler event="OnLButtonUp" function="EA_Window_Inspection.OnTabSelectChar" />
                            </EventHandlers>
                            <TextOffset x="0" y="5" />
                        </Button>
                        <Button name="$parentBragsTab" inherits="EA_Button_BottomTab">
                            <Anchors>
                                <Anchor point="topleft" relativePoint="topright" relativeTo="$parentCharTab">
                                    <AbsPoint x="0" y="0" />
                                </Anchor>
                            </Anchors>
                            <EventHandlers>
                                <EventHandler event="OnLButtonUp" function="EA_Window_Inspection.OnTabSelectBrags" />
                            </EventHandlers>
                            <TextOffset x="0" y="5" />
                        </Button>
                    </Windows>
                </Window>
            
            </Windows>
        </Window>
    </Windows>
</Interface>