	    <Label name="TOKEntryTitle" inherits="TOKSubHeading" textalign="left" >
        	<Size>
				<AbsPoint x="400" y="0"/>
			</Size>  
	        <Windows>
		    	<HorizontalResizeImage name="$parentTitleRule" inherits="TomeResizableHorizontalRule" >
		            <Anchors>
			            <Anchor point="bottom" relativePoint="bottom" relativeTo="">
				            <AbsPoint x="0" y="4"/>
			            </Anchor>
		            </Anchors>
	            </HorizontalResizeImage>		    
		    </Windows>
	    </Label>
	    
