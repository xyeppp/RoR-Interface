        <Window name="EA_Window_ParchmentCheckButton">
            <Size>
                <AbsPoint x="280" y="32" />
            </Size>
            <EventHandlers>
                <EventHandler event="OnInitialize" function="EA_LabelCheckButton.Initialize" />
                <EventHandler event="OnLButtonUp" function="EA_LabelCheckButton.Toggle" />
            </EventHandlers>
            <Windows>
                <Button name="$parentButton" inherits="EA_Button_DefaultCheckBox" handleinput="false">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft">
                            <AbsPoint x="0" y="0"/>
                        </Anchor>
                    </Anchors>
                </Button>
                <Label name="$parentLabel" inherits="EA_CheckButtonLabel" font="font_journal_text" handleinput="false" textAutoFitMinScale="0.5">
                    <Size>
                        <AbsPoint x="255" y="16" />
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="35" y="0" />
                        </Anchor>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                    <Color r="0" g="0" b="0" />
                </Label>
            </Windows>
        </Window>



        <Button name="EA_Button_ParchmentCheckButton" inherits="EA_Button_Default"
              backgroundtexture="EA_BigMap01_d5" highlighttexture="EA_BigMap01_d5" drawchildrenfirst="true">
            <Size>
                <AbsPoint x="35" y="25" />
            </Size>
            <TexSlices>
                <NormalHighlit id="Checkbox-ROLLOVER" />
                <Pressed id="Checkbox-CHECK" />
                <PressedHighlit id="Checkbox-ROLLOVER" />
            </TexSlices>
            <Windows>
                <DynamicImage name="$parentBox" texture="EA_BigMap01_d5" slice="Checkbox" handleinput="false" >
                    <Size>
                        <AbsPoint x="35" y="25" />
                    </Size>
                    <TexDims x="35" y="25" />
                </DynamicImage>
            </Windows>
        </Button>

        <Window name="WorldMapCheckButton" >
            <Size>
                <AbsPoint x="325" y="25" />
            </Size>
            <Windows>
                <Button name="$parentCheckBox" inherits="EA_Button_ParchmentCheckButton"  handleinput="false">
                    <Anchors>
                        <Anchor point="left" relativePoint="left" relativeTo="">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Button>
                <Label name="$parentName" inherits="TOKTextSmall" wordwrap="false" handleinput="false" textalign="leftcenter" >
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="45" y="0" />
                        </Anchor>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Label>
            </Windows>
        </Window>
        
    </Windows>
</Interface>