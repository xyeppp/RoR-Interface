<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
    <Windows>
          
	   <!--# Button: TomeCloseButton                   
           This is the close button style used for the TomeWindow and MapWindow.
        -->
        <Button name="TomeCloseButton" inherits="DefaultButton" backgroundtexture="tome_2" highlighttexture="tome_2"  layer="popup">
            <Size>
                <AbsPoint x="48" y="48"/>
            </Size>
            <Anchors>
                <Anchor point="topright" relativePoint="topright">
                    <AbsPoint x="0" y="0" />
                </Anchor>
            </Anchors>
            <TexCoords>
                <Normal x="219" y="126" />
                <NormalHighlit x="270" y="127" />
                <Pressed x="270" y="125" />
                <PressedHighlit x="270" y="127" />
                <Disabled x="219" y="126" />
            </TexCoords>
        </Button>
	  
		<Button name="OpenTomeButton" backgroundtexture="assorted_hud_pieces" highlighttexture="assorted_hud_pieces" layer="popup">
			<Size>
				<AbsPoint x="32" y="26" />
			</Size>
			<TexCoords>
				<Normal x="39" y="145" />
				<NormalHighlit x="39" y="145" />
				<Pressed x="39" y="145" />
				<PressedHighlit x="39" y="145" />
				<Disabled x="39" y="145" />
			</TexCoords>
		</Button>

		<Button name="TomePreviousPageButton" backgroundtexture="EA_Tome" highlighttexture="EA_Tome" layer="popup" texturescale="1.18">
			<Size>
				<AbsPoint x="114" y="89" />
			</Size>
            <TexSlices>
                <Normal          id="PageTurnLeft" />
                <NormalHighlit   id="PageTurnLeft-ROLLOVER" />
                <Pressed         id="PageTurnLeft-ROLLOVER" />
                <PressedHighlit  id="PageTurnLeft-ROLLOVER" />
                <Disabled        id="PageTurnLeft" />
                <DisabledPressed id="PageTurnLeft" />
            </TexSlices>
		</Button>

		<Button name="TomeNextPageButton" backgroundtexture="EA_Tome" highlighttexture="EA_Tome" layer="popup" texturescale="1.18">
			<Size>
				<AbsPoint x="114" y="89" />
			</Size>
            <TexSlices>
                <Normal          id="PageTurnRight" />
                <NormalHighlit   id="PageTurnRight-ROLLOVER" />
                <Pressed         id="PageTurnRight-ROLLOVER" />
                <PressedHighlit  id="PageTurnRight-ROLLOVER" />
                <Disabled        id="PageTurnRight" />
                <DisabledPressed id="PageTurnRight" />
            </TexSlices>
		</Button>

        
