                <Middle id="Map-Name-Scroll-Center"  />
                <Right id="Map-Name-Scroll-RightEnd"  />
            </TexSlices>
        </HorizontalResizeImage>
               
         
       
        <Window name="WorldMapScrollworkBorder" skipinput="true" layer="secondary" >
            <Size>
                <AbsPoint x="1075" y="1110" />
            </Size>
            <Windows>
                <DynamicImage name="$parentTopLeft" inherits="WorldMapScrollWorkTopLeft" handleinput="false">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft">
                            <AbsPoint x="15" y="55" />
                        </Anchor>
                    </Anchors>
                </DynamicImage>
                <DynamicImage name="$parentTopRight" inherits="WorldMapScrollWorkTopRight" handleinput="false">
                    <Anchors>
                        <Anchor point="topright" relativePoint="topright" relativeTo="$parent">
                            <AbsPoint x="-15" y="55" />
                        </Anchor>
                    </Anchors>
                </DynamicImage>
                
                <HorizontalResizeImage name="$parentTitleBackground" inherits="WorldMapScrollTitleBackground" handleinput="false">
                    <Size>
                        <AbsPoint x="700" y="85" />
                    </Size>
                    <Anchors>
                        <Anchor point="top" relativePoint="top" relativeTo="">
                            <AbsPoint x="15" y="10" />
                        </Anchor>
                    </Anchors>                
                </HorizontalResizeImage>
                
                <Label name="$parentTitleText" inherits="TOKHeading" layer="popup" textalign="center" textAutoFitMinScale="0.5" autoresize="false" wordwrap="false" handleinput="true">
                    <Size>
                        <AbsPoint x="622" y="74" />
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parentTitleBackground">
                            <AbsPoint x="142" y="20" />
                        </Anchor>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentTitleBackground">
                            <AbsPoint x="-142" y="-20" />
                        </Anchor>
                    </Anchors>
                </Label>
                
                <HorizontalResizeImage name="$parentTopMiddleLeft" inherits="WorldMapScrollWorkHorizResize" handleinput="false">
                    <Size>
                        <AbsPoint x="100" y="29" />
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentTopLeft">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                        <Anchor point="topleft" relativePoint="topright" relativeTo="$parentTitleBackground">
                            <AbsPoint x="0" y="45" />
                        </Anchor>
                    </Anchors>
                </HorizontalResizeImage>             
             
                <HorizontalResizeImage name="$parentTopMiddleRight" inherits="WorldMapScrollWorkHorizResize" handleinput="false">
                    <Size>
                        <AbsPoint x="20" y="29" />
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentTitleBackground">
                            <AbsPoint x="0" y="45" />
                        </Anchor>
                        <Anchor point="topleft" relativePoint="topright" relativeTo="$parentTopRight">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </HorizontalResizeImage>
                
                <VerticalResizeImage name="$parentLeftMiddle" inherits="WorldMapScrollWorkVertResize" handleinput="false">
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentTopLeft">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                        <Anchor point="topleft" relativePoint="bottomleft" relativeTo="$parentBottomLeft">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </VerticalResizeImage>
                <VerticalResizeImage name="$parentRightMiddle" inherits="WorldMapScrollWorkVertResize" handleinput="false">
                    <Anchors>
                        <Anchor point="bottomright" relativePoint="topright" relativeTo="$parentTopRight">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                        <Anchor point="topright" relativePoint="bottomright" relativeTo="$parentBottomRight">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </VerticalResizeImage>
                <DynamicImage name="$parentBottomLeft"  inherits="WorldMapScrollWorkBottomLeft" handleinput="false">
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="bottomleft">
                            <AbsPoint x="15" y="-10" />
                        </Anchor>
                    </Anchors>
                </DynamicImage>
                <DynamicImage name="$parentBottomRight" inherits="WorldMapScrollWorkBottomRight" handleinput="false">
                    <Anchors>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent">
                            <AbsPoint x="-15" y="-10" />
                        </Anchor>
                    </Anchors>
                </DynamicImage>
                <HorizontalResizeImage name="$parentBottomMiddle" inherits="WorldMapScrollWorkHorizResize" handleinput="false">
                    <Anchors>
                        <Anchor point="bottomright" relativePoint="bottomleft" relativeTo="$parentBottomLeft">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                        <Anchor point="bottomleft" relativePoint="bottomright" relativeTo="$parentBottomRight">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </HorizontalResizeImage>
            </Windows>
        </Window>

        <!-- Only the scrollwork, no knobs -->
