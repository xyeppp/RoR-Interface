       <HorizontalResizeImage name="WorldMapScrollTitleBackground" texture="EA_BigMap01_d5">
            <Size>
                <AbsPoint x="0" y="85" />
            </Size>
            <Sizes left="162" middle="294" right="162" />
            <TexSlices>
